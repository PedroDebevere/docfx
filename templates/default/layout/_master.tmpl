{{!include(/^styles/.*/)}}
{{!include(/^fonts/.*/)}}
{{!include(favicon.ico)}}
{{!include(logo.svg)}}
{{!include(search-stopwords.json)}}
<!DOCTYPE html>
<html>
  {{>partials/head}}
  {{^redirect_url}}
  <body>
    <header>
      {{^_disableNavbar}}
      {{>partials/navbar}}
      {{/_disableNavbar}}
    </header>

    {{#_enableSearch}}
    <div class="container">
      {{>partials/searchResults}}
    </div>
    {{/_enableSearch}}

    <main class="container hide-when-search">
      {{^_disableToc}}
      {{>partials/toc}}
      {{/_disableToc}}

      {{^_disableBreadcrumb}}
      {{>partials/breadcrumb}}
      {{/_disableBreadcrumb}}
      
      {{^_disableAffix}}
      {{>partials/affix}}
      {{/_disableAffix}}

      <article class="content wrap" id="_content" data-uid="{{uid}}">
        {{!body}}
      </article>
    </main>

    {{^_disableFooter}}
    {{>partials/footer}}
    {{/_disableFooter}}

    {{>partials/scripts}}
  </body>
  {{/redirect_url}}
</html>
